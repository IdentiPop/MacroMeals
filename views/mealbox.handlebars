
<div class = "row">
  <div class = "col s1"></div>
  <div class = "col s10" id="mealbox">
    <div class="row">
    <div class="col s12 m6">
      <div class="card">
        <div class="card-image">
          <img src="{{this.image}}">
          <span class="card-title">{{this.title}}</span>
          <a class="btn-floating halfway-fab waves-effect waves-light red sharebtn"><i class="material-icons">share</i></a>
          <a class="btn-floating halfway-fab waves-effect waves-light red deletebtn"><i class="material-icons">delete</i></a>
        </div>
        <div class="card-content">
          <p>Calories: {{this.calories}}</p><br>
          <p>Protein: {{this.protein}}</p><br>
          <p>Carbs: {{this.carbs}}</p><br>
          <p>Fat: {{this.fat}}</p><br>
          <a id = "recipelink" href={{this.link}}>Recipe Link</a>

        </div>
      </div>
    </div>
  </div>
  </div>
  <div class = "col s1"></div>
</div>

</main>
